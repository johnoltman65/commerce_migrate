        }
            },
    "extra": {
        "patches": {
            "drupal/commerce": {
                "Error: Call to undefined method Drupal\\commerce_order\\Entity\\OrderItem::getCustomer()": "https://www.drupal.org/files/issues/2020-07-07/3137651-8.patch"
            }
        }
    }
